{
    "name": "nih",
    "version": "0.1.0",
    "description": "Accessibility audit for trinidadstate.edu",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "rollup": "node rollup.js",
        "testm": "TODAY=$(date \"+%Y-%m-%d\"); unlighthouse-ci --site https://trinidadstate.edu --mobile  --output-path reports/mobile/$TODAY ",
        "testd": "TODAY=$(date \"+%Y-%m-%d\"); unlighthouse-ci --site https://trinidadstate.edu --desktop --output-path reports/desktop/$TODAY ",
        "summary": "node summary.js",
        "test": "npm run testm; npm run testd; npm run summary"
    },
    "author": "Scott Davis <scott.davis@frontrange.edu>",
    "license": "CC-BY-4.0",
    "dependencies": {
        "walker": "^1.0.8"
    }
}